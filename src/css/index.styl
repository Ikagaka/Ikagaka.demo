html, body
	width 100%
	height 100%
	margin 0
	padding 0

a
	color #06c

.ConsoleWindow
	position fixed
	top 0
	left 0
	width 40em
	height 20em
	background rgba(255,255,255,0.5)
	overflow auto
	font-size small
	.log
		color #090
	.warn
		color #f66
	.error
		color #f00


//.namedMgr
//	position fixed
//	bottom 0
//	left 0
//	width 100%
//	height 0

header
	margin 0.5em
	padding 0.5em
	border-bottom 1px solid #ccc
	h1
		margin 0
		a
			text-decoration none
article
	margin 0.5em
	line-height 140%
	.nar
		label
			display block
			max-width 20em
			margin 0.5em auto
			padding 2em
			text-align center
			color #fff
			background #06f
			input[type="file"]
				display none
		&:after
			content ""
			display block
			clear both
		.nanikas
			list-style none
			max-width 20em
			margin 0.5em auto
			padding 0em
			*
				display block
				margin 0.5em auto
				padding 0.5em
				background #ccc
				cursor pointer
	h2
		margin 0.5em 0
		padding 0.5em 0
		border-bottom 1px solid #ddd
	p
		margin 0.5em
	ul
		margin 0
		padding 0 0 0 1.5em
		li
			margin 0.5em
	dl
		margin 0
		padding 0
		dt
			margin 0
			padding 0
			font-weight bold
		dd
			margin 0
			padding 0 0 0 1.5em
footer
	margin 0.5em
